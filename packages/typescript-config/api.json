{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node/Express with JSX (API)",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["node", "react", "react/jsx-runtime"],
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
